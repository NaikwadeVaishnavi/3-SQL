SELECT * FROM FILM

SELECT DISTINCT(FILM_ID) FROM FILM

SELECT * FROM FILM ORDER BY FILM_ID

SELECT * FROM FILM ORDER BY FILM_ID LIMIT 5

SELECT * FROM FILM WHERE FILM_ID BETWEEN 1 AND 10 

SELECT * FROM FILM WHERE FILM_ID IN (2,3,4)

SELECT * FROM FILM WHERE TITLE ilIKE 'A%'

SELECT COUNT(*) FROM FILM WHERE TITLE LIKE '%a'
