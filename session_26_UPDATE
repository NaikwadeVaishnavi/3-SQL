UPDATE ACCOUNT SET USERNAME ='GITA' WHERE USER_ID=1;

UPDATE ACCOUNT SET LAST_LOGIN = CURRENT_TIMESTAMP

-----UPDATE JOIN
SELECT * FROM JOB
SELECT * FROM ACCOUNT_JOB

UPDATE ACCOUNT_JOB SET HAIR_DATE = ACCOUNT.CREATED_ON FROM ACCOUNT
WHERE ACCOUNT_JOB.USER_ID=ACCOUNT.USER_ID

UPDATE ACCOUNT SET LAST_LOGIN=CURRENT_TIMESTAMP
RETURNING EMAIL,CREATED_ON,LAST_LOGIN --------------FOR OUTPUT


